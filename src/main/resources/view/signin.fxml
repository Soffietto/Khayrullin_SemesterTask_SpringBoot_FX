<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.kpfu.itis.khayrullin.controller.SignInController">
   <children>
      <Label layoutX="60.0" layoutY="28.0" prefHeight="15.0" prefWidth="511.0" text="Azat's Car Rental Service" textFill="#da3030">
         <font>
            <Font name="DejaVuMathTeXGyre-Regular" size="39.0" />
         </font>
      </Label>
      <TextField fx:id="et_login" layoutX="300.0" layoutY="167.0" prefHeight="33.0" prefWidth="205.0" promptText="Login" />
      <PasswordField fx:id="et_password" layoutX="300.0" layoutY="223.0" prefHeight="33.0" prefWidth="205.0" promptText="Password" />
      <Button layoutX="300.0" layoutY="270.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="97.0" text="Login" onAction="#loginButton" />
      <Button layoutX="403.0" layoutY="270.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="97.0" text="Registration" onAction="#toSignUpButton"/>
      <ImageView fx:id="et_image" fitHeight="168.0" fitWidth="236.0" layoutX="48.0" layoutY="139.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="/img/rent.png" />
         </image>
      </ImageView>
      <Label layoutX="338.0" layoutY="107.0" prefHeight="45.0" prefWidth="119.0" text="Sign In" textAlignment="CENTER" textFill="#794a4a">
         <font>
            <Font size="33.0" />
         </font>
      </Label>
   </children>
   <cursor>
      <Cursor fx:constant="DEFAULT" />
   </cursor>
</AnchorPane>
